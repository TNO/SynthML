controllable left;
controllable right;
controllable __start;
controllable __end;
automaton minimal:
  alphabet left, right, __start, __end;
  @state(projected: "s1")
  location s1:
    initial;
    edge __start goto s2;
  @state(projected: "s2")
  location s2:
    edge right goto s4;
    edge left goto s4;
  @state(projected: "s5")
  location s3:
    marked;
  @state(projected: "s4")
  @state(projected: "s3")
  location s4:
    edge __end goto s3;
end
