controllable flip;
controllable initialize;
controllable __start;
controllable __end;
automaton projected:
  alphabet flip, initialize, __start, __end;
  @state(statespace = "__init")
  location s1:
    initial;
    edge __start goto s2;
  @state(statespace = "loc2")
  @state(statespace = "loc1")
  location s2:
    edge initialize goto s3;
  @state(statespace = "loc4")
  @state(statespace = "loc3")
  @state(statespace = "loc6")
  @state(statespace = "loc5")
  location s3:
    edge __end goto s4;
    edge flip goto s5;
  @state(statespace = "__done")
  location s4:
    marked;
  @state(statespace = "loc5")
  location s5:
    edge __end goto s4;
end
