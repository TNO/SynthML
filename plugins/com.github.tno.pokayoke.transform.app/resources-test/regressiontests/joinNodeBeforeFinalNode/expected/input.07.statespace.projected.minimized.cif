controllable action1;
controllable action2;
controllable __start;
controllable __end;
automaton minimal:
  alphabet action1, action2, __start, __end;
  @state(projected = "s1")
  location s1:
    initial;
    edge __start goto s2;
  @state(projected = "s2")
  location s2:
    edge action2 goto s4;
    edge action1 goto s5;
  @state(projected = "s7")
  location s3:
    marked;
  @state(projected = "s3")
  location s4:
    edge action1 goto s6;
  @state(projected = "s4")
  location s5:
    edge action2 goto s6;
    edge action1;
  @state(projected = "s5")
  @state(projected = "s6")
  location s6:
    edge __end goto s3;
    edge action1;
end
