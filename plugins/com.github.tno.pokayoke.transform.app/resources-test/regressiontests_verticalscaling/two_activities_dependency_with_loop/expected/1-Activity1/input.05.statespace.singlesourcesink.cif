controllable flipProp1;
controllable flipExtra;
controllable __start;
controllable __end;
automaton synthesis_state_space:
  alphabet flipProp1, flipExtra, __start, __end;
  @state(Class1 = "*", Class1.extra = false, Class1.property1 = false, Class1.property2 = false, sup = "*")
  location __init:
    initial;
    edge __start goto loc1;
  @state(Class1 = "*", Class1.extra = false, Class1.property1 = false, Class1.property2 = false, sup = "*")
  location loc1:
    edge flipProp1 goto loc2;
    edge flipExtra goto loc3;
  @state(Class1 = "*", Class1.extra = false, Class1.property1 = true, Class1.property2 = false, sup = "*")
  location loc2:
    edge flipProp1 goto loc1;
    edge flipExtra goto loc4;
  @state(Class1 = "*", Class1.extra = true, Class1.property1 = false, Class1.property2 = false, sup = "*")
  location loc3:
    edge flipProp1 goto loc4;
  @state(Class1 = "*", Class1.extra = true, Class1.property1 = true, Class1.property2 = false, sup = "*")
  location loc4:
    edge __end goto __done;
  @state(Class1 = "*", Class1.extra = true, Class1.property1 = true, Class1.property2 = false, sup = "*")
  location __done:
    marked;
end
