controllable left;
controllable right;
controllable flip;
controllable __start;
controllable __end;
automaton projected:
  alphabet left, right, flip, __start, __end;
  @state(statespace = "__init")
  location s1:
    initial;
    edge __start goto s2;
  @state(statespace = "loc2")
  @state(statespace = "loc1")
  location s2:
    edge flip goto s3;
    edge right goto s4;
    edge left goto s5;
  @state(statespace = "loc6")
  @state(statespace = "loc4")
  location s3:
    edge right goto s8;
    edge left goto s9;
  @state(statespace = "loc5")
  location s4:
    edge flip goto s7;
  @state(statespace = "loc3")
  location s5:
    edge flip goto s6;
  @state(statespace = "loc7")
  location s6:
    edge __end goto s10;
  @state(statespace = "loc9")
  location s7:
    edge __end goto s10;
  @state(statespace = "loc8")
  location s8:
    edge __end goto s10;
  @state(statespace = "loc10")
  location s9:
    edge __end goto s10;
  @state(statespace = "__done")
  location s10:
    marked;
end
