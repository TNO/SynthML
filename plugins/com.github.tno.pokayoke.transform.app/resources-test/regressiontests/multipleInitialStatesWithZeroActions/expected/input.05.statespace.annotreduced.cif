controllable __start;
controllable __end;
automaton statespace:
  alphabet __start, __end;
  @state(Context = "*", Context.field = false, sup = "*")
  @state(Context = "*", Context.field = true, sup = "*")
  location __init:
    initial;
    edge __start goto loc1;
    edge __start goto loc2;
  @state(Context = "*", Context.field = false, sup = "*")
  location loc1:
    edge __end goto __done;
  @state(Context = "*", Context.field = true, sup = "*")
  location loc2:
    edge __end goto __done;
  @state(Context = "*", Context.field = false, sup = "*")
  @state(Context = "*", Context.field = true, sup = "*")
  location __done:
    marked;
end
