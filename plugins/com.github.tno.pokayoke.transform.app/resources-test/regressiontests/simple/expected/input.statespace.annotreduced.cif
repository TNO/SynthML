group Context:
  controllable prepare_robot;
  controllable robot_move_to_location1;
  controllable robot_move_to_location2;
  controllable robot_adjust_product;
  controllable robot_release_product_at_location2;
  controllable prepare_location2;
  controllable measure_product_position;
  uncontrollable measure_product_position_result_1;
  uncontrollable measure_product_position_result_2;
end
group Post:
  controllable c_satisfied;
end
automaton statespace:
  alphabet Context.measure_product_position, Context.measure_product_position_result_1, Context.measure_product_position_result_2, Context.prepare_location2, Context.prepare_robot, Context.robot_adjust_product, Context.robot_move_to_location1, Context.robot_move_to_location2, Context.robot_release_product_at_location2, Post.c_satisfied;
  @state(constraint = "idle", Context = "*", Context.location2HasProduct = false, Context.location2Prepared = false, Context.productCorrectlyPlaced = "Unknown", Context.robotHasProduct = true, Context.robotLocation = "Location1", Context.robotPrepared = false, Post = "NotSatisfied", Prepare_Location2_happens_once = "NotYetOccurred", Prepare_robot_happens_once = "NotYetOccurred", Robot_move_to_Location2_happens_once = "NotYetOccurred", Robot_release_product_at_location2_happens_once = "NotYetOccurred", sup = "*")
  location loc1:
    initial;
    edge Context.prepare_location2 goto loc2;
    edge Context.prepare_robot goto loc3;
  @state(constraint = "idle", Context = "*", Context.location2HasProduct = false, Context.location2Prepared = true, Context.productCorrectlyPlaced = "Unknown", Context.robotHasProduct = true, Context.robotLocation = "Location1", Context.robotPrepared = false, Post = "NotSatisfied", Prepare_Location2_happens_once = "Occurred", Prepare_robot_happens_once = "NotYetOccurred", Robot_move_to_Location2_happens_once = "NotYetOccurred", Robot_release_product_at_location2_happens_once = "NotYetOccurred", sup = "*")
  location loc2:
    edge Context.prepare_robot goto loc4;
  @state(constraint = "idle", Context = "*", Context.location2HasProduct = false, Context.location2Prepared = false, Context.productCorrectlyPlaced = "Unknown", Context.robotHasProduct = true, Context.robotLocation = "Location1", Context.robotPrepared = true, Post = "NotSatisfied", Prepare_Location2_happens_once = "NotYetOccurred", Prepare_robot_happens_once = "Occurred", Robot_move_to_Location2_happens_once = "NotYetOccurred", Robot_release_product_at_location2_happens_once = "NotYetOccurred", sup = "*")
  location loc3:
    edge Context.prepare_location2 goto loc4;
  @state(constraint = "idle", Context = "*", Context.location2HasProduct = false, Context.location2Prepared = true, Context.productCorrectlyPlaced = "Unknown", Context.robotHasProduct = true, Context.robotLocation = "Location1", Context.robotPrepared = true, Post = "NotSatisfied", Prepare_Location2_happens_once = "Occurred", Prepare_robot_happens_once = "Occurred", Robot_move_to_Location2_happens_once = "NotYetOccurred", Robot_release_product_at_location2_happens_once = "NotYetOccurred", sup = "*")
  location loc4:
    edge Context.robot_move_to_location2 goto loc5;
  @state(constraint = "idle", Context = "*", Context.location2HasProduct = false, Context.location2Prepared = true, Context.productCorrectlyPlaced = "Unknown", Context.robotHasProduct = true, Context.robotLocation = "Location2", Context.robotPrepared = true, Post = "NotSatisfied", Prepare_Location2_happens_once = "Occurred", Prepare_robot_happens_once = "Occurred", Robot_move_to_Location2_happens_once = "Occurred", Robot_release_product_at_location2_happens_once = "NotYetOccurred", sup = "*")
  location loc5:
    edge Context.robot_release_product_at_location2 goto loc6;
  @state(constraint = "idle", Context = "*", Context.location2HasProduct = true, Context.location2Prepared = true, Context.productCorrectlyPlaced = "Unknown", Context.robotHasProduct = false, Context.robotLocation = "Location2", Context.robotPrepared = true, Post = "NotSatisfied", Prepare_Location2_happens_once = "Occurred", Prepare_robot_happens_once = "Occurred", Robot_move_to_Location2_happens_once = "Occurred", Robot_release_product_at_location2_happens_once = "Occurred", sup = "*")
  location loc6:
    edge Context.measure_product_position goto loc7;
  location loc7:
    edge Context.measure_product_position_result_1 goto loc8;
    edge Context.measure_product_position_result_2 goto loc9;
  @state(constraint = "idle", Context = "*", Context.location2HasProduct = true, Context.location2Prepared = true, Context.productCorrectlyPlaced = "True", Context.robotHasProduct = false, Context.robotLocation = "Location2", Context.robotPrepared = true, Post = "NotSatisfied", Prepare_Location2_happens_once = "Occurred", Prepare_robot_happens_once = "Occurred", Robot_move_to_Location2_happens_once = "Occurred", Robot_release_product_at_location2_happens_once = "Occurred", sup = "*")
  location loc8:
    edge Context.robot_move_to_location1 goto loc10;
  @state(constraint = "idle", Context = "*", Context.location2HasProduct = true, Context.location2Prepared = true, Context.productCorrectlyPlaced = "False", Context.robotHasProduct = false, Context.robotLocation = "Location2", Context.robotPrepared = true, Post = "NotSatisfied", Prepare_Location2_happens_once = "Occurred", Prepare_robot_happens_once = "Occurred", Robot_move_to_Location2_happens_once = "Occurred", Robot_release_product_at_location2_happens_once = "Occurred", sup = "*")
  location loc9:
    edge Context.robot_adjust_product goto loc6;
  @state(constraint = "idle", Context = "*", Context.location2HasProduct = true, Context.location2Prepared = true, Context.productCorrectlyPlaced = "True", Context.robotHasProduct = false, Context.robotLocation = "Location1", Context.robotPrepared = true, Post = "NotSatisfied", Prepare_Location2_happens_once = "Occurred", Prepare_robot_happens_once = "Occurred", Robot_move_to_Location2_happens_once = "Occurred", Robot_release_product_at_location2_happens_once = "Occurred", sup = "*")
  location loc10:
    edge Post.c_satisfied goto loc11;
  @state(constraint = "idle", Context = "*", Context.location2HasProduct = true, Context.location2Prepared = true, Context.productCorrectlyPlaced = "True", Context.robotHasProduct = false, Context.robotLocation = "Location1", Context.robotPrepared = true, Post = "Satisfied", Prepare_Location2_happens_once = "Occurred", Prepare_robot_happens_once = "Occurred", Robot_move_to_Location2_happens_once = "Occurred", Robot_release_product_at_location2_happens_once = "Occurred", sup = "*")
  location loc11:
    marked;
end
