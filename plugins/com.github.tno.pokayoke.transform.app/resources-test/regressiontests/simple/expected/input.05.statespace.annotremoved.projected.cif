group Context:
  controllable prepare_robot;
  controllable robot_move_to_location1;
  controllable robot_move_to_location2;
  controllable robot_adjust_product;
  controllable robot_release_product_at_location2;
  controllable prepare_location2;
  controllable measure_product_position;
end
group Post:
  controllable c_satisfied;
end
automaton projected:
  alphabet Context.prepare_robot, Context.robot_move_to_location1, Context.robot_move_to_location2, Context.robot_adjust_product, Context.robot_release_product_at_location2, Context.prepare_location2, Context.measure_product_position, Post.c_satisfied;
  @state(statespace = "loc1")
  location s1:
    initial;
    edge Context.prepare_location2 goto s2;
    edge Context.prepare_robot goto s3;
  @state(statespace = "loc2")
  location s2:
    edge Context.prepare_robot goto s4;
  @state(statespace = "loc3")
  location s3:
    edge Context.prepare_location2 goto s4;
  @state(statespace = "loc4")
  location s4:
    edge Context.robot_move_to_location2 goto s5;
  @state(statespace = "loc5")
  location s5:
    edge Context.robot_release_product_at_location2 goto s6;
  @state(statespace = "loc6")
  location s6:
    edge Context.measure_product_position goto s7;
  @state(statespace = "loc7")
  @state(statespace = "loc9")
  @state(statespace = "loc8")
  location s7:
    edge Context.robot_adjust_product goto s6;
    edge Context.robot_move_to_location1 goto s8;
  @state(statespace = "loc10")
  location s8:
    edge Post.c_satisfied goto s9;
  @state(statespace = "loc11")
  location s9:
    marked;
end
