controllable left;
controllable right;
controllable __start;
controllable __end;
automaton statespace:
  alphabet left, right, __start, __end;
  location __init:
    initial;
    edge __start goto loc1;
    edge __start goto loc2;
  location loc1:
    edge left goto loc3;
  location loc2:
    edge right goto loc4;
  location loc3:
    edge __end goto __done;
  location loc4:
    edge __end goto __done;
  location __done:
    marked;
end
