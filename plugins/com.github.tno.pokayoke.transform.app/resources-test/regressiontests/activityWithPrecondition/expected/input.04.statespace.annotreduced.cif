controllable change;
automaton statespace:
  alphabet change;
  @state(Class = "*", Class.element = "First", happens_at_most_once__change = "*", happens_at_most_once__change.occurrences = 0, sup = "*")
  location loc1:
    initial;
    edge change goto loc2;
  @state(Class = "*", Class.element = "Second", happens_at_most_once__change = "*", happens_at_most_once__change.occurrences = 1, sup = "*")
  location loc2:
    marked;
end
