controllable flipProp1;
controllable flipExtra;
controllable __start;
controllable __end;
automaton minimal:
  alphabet flipProp1, flipExtra, __start, __end;
  @state(projected = "s1")
  location s1:
    initial;
    edge __start goto s2;
  @state(projected = "s2")
  location s2:
    edge flipExtra goto s4;
    edge flipProp1 goto s5;
  @state(projected = "s6")
  location s3:
    marked;
  @state(projected = "s3")
  location s4:
    edge flipProp1 goto s6;
  @state(projected = "s4")
  location s5:
    edge flipExtra goto s6;
    edge flipProp1 goto s2;
  @state(projected = "s5")
  location s6:
    edge __end goto s3;
end
