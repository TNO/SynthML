group Context:
  controllable prepare_robot;
  controllable robot_move_to_location1;
  controllable robot_move_to_location2;
  controllable robot_adjust_product;
  controllable robot_release_product_at_location2;
  controllable prepare_location2;
  controllable measure_product_position;
end
group Post:
  controllable c_satisfied;
end
automaton minimal:
  alphabet Context.prepare_robot, Context.robot_move_to_location1, Context.robot_move_to_location2, Context.robot_adjust_product, Context.robot_release_product_at_location2, Context.prepare_location2, Context.measure_product_position, Post.c_satisfied;
  @state(projected = "s1")
  location s1:
    initial;
    edge Context.prepare_location2 goto s2;
    edge Context.prepare_robot goto s3;
  @state(projected = "s2")
  location s2:
    edge Context.prepare_robot goto s5;
  @state(projected = "s3")
  location s3:
    edge Context.prepare_location2 goto s5;
  @state(projected = "s9")
  location s4:
    marked;
  @state(projected = "s4")
  location s5:
    edge Context.robot_move_to_location2 goto s9;
  @state(projected = "s7")
  location s6:
    edge Context.robot_adjust_product goto s7;
    edge Context.robot_move_to_location1 goto s8;
  @state(projected = "s6")
  location s7:
    edge Context.measure_product_position goto s6;
  @state(projected = "s8")
  location s8:
    edge Post.c_satisfied goto s4;
  @state(projected = "s5")
  location s9:
    edge Context.robot_release_product_at_location2 goto s7;
end
