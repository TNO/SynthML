controllable change;
controllable __start;
controllable __end;
automaton projected:
  alphabet change, __start, __end;
  @state(statespace = "__init")
  location s1:
    initial;
    edge __start goto s2;
  @state(statespace = "loc1")
  location s2:
    edge change goto s3;
  @state(statespace = "loc2")
  location s3:
    edge __end goto s4;
  @state(statespace = "__done")
  location s4:
    marked;
end
