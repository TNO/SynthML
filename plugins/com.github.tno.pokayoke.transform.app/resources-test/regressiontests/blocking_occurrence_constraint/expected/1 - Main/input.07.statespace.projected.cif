controllable left;
controllable right;
controllable flip;
controllable __start;
controllable __end;
automaton projected:
  alphabet left, right, flip, __start, __end;
  @state(statespace = "__init")
  location s1:
    initial;
    edge __start goto s2;
  @state(statespace = "loc2")
  @state(statespace = "loc1")
  location s2:
    edge right goto s3;
    edge left goto s4;
  @state(statespace = "loc4")
  location s3:
    edge __end goto s5;
  @state(statespace = "loc3")
  location s4:
    edge __end goto s5;
  @state(statespace = "__done")
  location s5:
    marked;
end
