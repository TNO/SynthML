controllable Turn_Computer_On;
controllable Starting_Computer;
controllable Drink_Coffee;
controllable Drink_Tea;
controllable Start_Working;
controllable Flip_A_Coin;
controllable c_satisfied;
automaton projected:
  alphabet Turn_Computer_On, Starting_Computer, Drink_Coffee, Drink_Tea, Start_Working, Flip_A_Coin, c_satisfied;
  @state(statespace = "loc1")
  location s1:
    initial;
    edge Turn_Computer_On goto s2;
  @state(statespace = "loc2")
  location s2:
    edge Flip_A_Coin goto s3;
    edge Starting_Computer goto s4;
  @state(statespace = "loc4")
  @state(statespace = "loc7")
  @state(statespace = "loc6")
  location s3:
    edge Drink_Tea goto s6;
    edge Drink_Coffee goto s7;
    edge Starting_Computer goto s8;
  @state(statespace = "loc3")
  location s4:
    edge Flip_A_Coin goto s5;
  @state(statespace = "loc5")
  @state(statespace = "loc9")
  @state(statespace = "loc8")
  location s5:
    edge Drink_Tea goto s9;
    edge Drink_Coffee goto s10;
  @state(statespace = "loc11")
  location s6:
    edge Starting_Computer goto s9;
  @state(statespace = "loc10")
  location s7:
    edge Starting_Computer goto s10;
  @state(statespace = "loc9")
  @state(statespace = "loc8")
  location s8:
    edge Drink_Tea goto s9;
    edge Drink_Coffee goto s10;
  @state(statespace = "loc13")
  location s9:
    edge Start_Working goto s12;
  @state(statespace = "loc12")
  location s10:
    edge Start_Working goto s11;
  @state(statespace = "loc14")
  location s11:
    edge c_satisfied goto s13;
  @state(statespace = "loc15")
  location s12:
    edge c_satisfied goto s14;
  @state(statespace = "loc16")
  location s13:
    marked;
  @state(statespace = "loc17")
  location s14:
    marked;
end
