enum Number = First, Second, Third;
controllable change;
plant automaton Class:
  disc Number element in any;
  alg bool precond = element = First;
  alg bool __precondition = precond;
  alg bool postcond = element = Second;
  alg bool __postcondition__happens_at_most_once__28__change__16 = happens_at_most_once__28__change__16.__occurrences >= 0;
  alg bool __postcondition = postcond and __postcondition__happens_at_most_once__28__change__16;
  initial __precondition;
  marked __postcondition;
  location:
    initial;
    marked;
    edge change when element != Third do element := Second;
end
requirement automaton happens_at_most_once__28__change__16:
  disc int[0..1] __occurrences;
  marked __occurrences >= 0;
  location:
    initial;
    marked;
    edge change when __occurrences < 1 do __occurrences := __occurrences + 1;
end
requirement invariant Class.__postcondition disables change;
