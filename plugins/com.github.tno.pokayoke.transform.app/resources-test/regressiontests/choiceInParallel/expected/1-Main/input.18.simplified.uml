      <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="58" name="Turn_Computer_On">
        <body>not Is_Computer_Ready and not Is_Computer_On</body>
        <body>Is_Computer_On := true</body>
      </ownedBehavior>
      <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="59" name="Starting_Computer">
        <body>not Is_Computer_Ready and Is_Computer_On</body>
        <body>Is_Computer_Ready := true</body>
      </ownedBehavior>
      <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="60" name="Drink_Coffee">
        <body>not Has_Drank_Coffee_or_Tea and Coffee_or_Tea = Coffee</body>
        <body>Has_Drank_Coffee_or_Tea := true, Coffee_or_Tea:= Unknown</body>
      </ownedBehavior>
      <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="61" name="Drink_Tea">
