controllable do_a;
uncontrollable do_a__result_1;
uncontrollable do_a__result_2;
controllable do_b;
uncontrollable do_b__na_result_1;
plant automaton Context:
  disc bool done_a in any;
  disc bool done_b in any;
  disc int[0..1] __activeAction;
  disc bool __nonAtomicActive__do_b;
  alg bool $pre = not done_a and not done_b;
  alg bool __precondition = $pre;
  alg bool $post = done_a;
  alg bool __postcondition = $post;
  initial __precondition;
  marked __postcondition;
  location:
    initial;
    marked;
    edge do_a when not done_a, __activeAction = 0 do __activeAction := 1;
    edge do_a__result_1 when __activeAction = 1 do done_a := true, __activeAction := 0;
    edge do_a__result_2 when __activeAction = 1 do done_a := true, __activeAction := 0;
    edge do_b when not done_b, __activeAction = 0, not __nonAtomicActive__do_b do __nonAtomicActive__do_b := true;
    edge do_b__na_result_1 when __activeAction = 0, __nonAtomicActive__do_b do done_b := true, __nonAtomicActive__do_b := false;
end
supervisor automaton occurrences__do_a:
  disc int[0..1] __occurrences;
  location:
    initial;
    marked __occurrences >= 0;
    edge do_a when __occurrences < 1 do __occurrences := __occurrences + 1;
end
supervisor automaton occurrences__do_b:
  disc int[0..1] __occurrences;
  location:
    initial;
    marked __occurrences >= 0;
    edge do_b when __occurrences < 1 do __occurrences := __occurrences + 1;
end
supervisor automaton sup:
  alphabet do_a, do_b;
  location:
    initial;
    marked;
    edge do_a when occurrences__do_a.__occurrences = 0 and (not Context.done_a and Context.__activeAction = 0) and (not Context.__nonAtomicActive__do_b and (not Context.done_b and occurrences__do_b.__occurrences = 0)) or occurrences__do_a.__occurrences = 0 and (not Context.done_a and Context.__activeAction = 0) and (not Context.__nonAtomicActive__do_b and (Context.done_b and occurrences__do_b.__occurrences = 1));
    edge do_b when occurrences__do_a.__occurrences = 0 and (not Context.done_a and Context.__activeAction = 0) and (not Context.__nonAtomicActive__do_b and (not Context.done_b and occurrences__do_b.__occurrences = 0));
end
