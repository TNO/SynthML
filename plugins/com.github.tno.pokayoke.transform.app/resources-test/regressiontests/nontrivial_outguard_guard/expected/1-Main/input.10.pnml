<pnml xmlns="http://www.pnml.org/version-2009/grammar/pnml">
  <net id="minimal" type="http://www.pnml.org/version-2009/grammar/ptnet">
    <page id="minimal">
      <transition id="__start">
        <name>
          <text>__start</text>
        </name>
      </transition>
      <transition id="flip">
        <name>
          <text>flip</text>
        </name>
      </transition>
      <transition id="right">
        <name>
          <text>right</text>
        </name>
      </transition>
      <transition id="left">
        <name>
          <text>left</text>
        </name>
      </transition>
      <transition id="__end">
        <name>
          <text>__end</text>
        </name>
      </transition>
      <transition id="__loop">
        <name>
          <text>__loop</text>
        </name>
      </transition>
      <place id="p6">
        <name>
          <text>p6</text>
        </name>
      </place>
      <arc id="__end__to__p6" source="__end" target="p6">
      </arc>
      <arc id="__loop__to__p6" source="__loop" target="p6">
      </arc>
      <place id="p2">
        <name>
          <text>p2</text>
        </name>
      </place>
      <place id="p3">
        <name>
          <text>p3</text>
        </name>
      </place>
      <arc id="__start__to__p2" source="__start" target="p2">
      </arc>
      <arc id="__start__to__p3" source="__start" target="p3">
      </arc>
      <place id="p4">
        <name>
          <text>p4</text>
        </name>
      </place>
      <arc id="flip__to__p4" source="flip" target="p4">
      </arc>
      <place id="p5">
        <name>
          <text>p5</text>
        </name>
      </place>
      <arc id="left__to__p5" source="left" target="p5">
      </arc>
      <place id="p1">
        <name>
          <text>p1</text>
        </name>
        <initialMarking>
          <text>1</text>
        </initialMarking>
      </place>
      <arc id="p1__to____start" source="p1" target="__start">
      </arc>
      <arc id="p2__to__flip" source="p2" target="flip">
      </arc>
      <arc id="p3__to__left" source="p3" target="left">
      </arc>
      <arc id="p3__to__right" source="p3" target="right">
      </arc>
      <arc id="p4__to____end" source="p4" target="__end">
      </arc>
      <arc id="p5__to____end" source="p5" target="__end">
      </arc>
      <arc id="p6__to____loop" source="p6" target="__loop">
      </arc>
      <arc id="right__to__p5" source="right" target="p5">
      </arc>
    </page>
  </net>
</pnml>
