enum Number = First, Second, Third;
controllable change;
plant automaton Class:
  disc Number element in any;
  initial element = First;
  marked element = Second;
  location:
    initial;
    marked;
    edge change when element != Third do element := Second;
end
supervisor automaton happens_at_most_once__change:
  disc int[0..1] occurrences;
  location:
    initial;
    marked occurrences >= 0;
    edge change when occurrences < 1 do occurrences := occurrences + 1;
end
supervisor automaton sup:
  alphabet change;
  location:
    initial;
    marked;
    edge change when Class.element = First and happens_at_most_once__change.occurrences = 0 or Class.element = Second and happens_at_most_once__change.occurrences = 0;
end
