controllable Turn_Computer_On;
controllable Starting_Computer;
controllable Drink_Coffee;
controllable Drink_Tea;
controllable Start_Working;
controllable Flip_A_Coin;
controllable __start;
controllable __end;
automaton minimal:
  alphabet Turn_Computer_On, Starting_Computer, Drink_Coffee, Drink_Tea, Start_Working, Flip_A_Coin, __start, __end;
  @state(projected: "s1")
  location s1:
    initial;
    edge __start goto s2;
  @state(projected: "s2")
  location s2:
    edge Turn_Computer_On goto s4;
  @state(projected: "s14")
  location s3:
    marked;
  @state(projected: "s3")
  location s4:
    edge Flip_A_Coin goto s5;
    edge Starting_Computer goto s6;
  @state(projected: "s4")
  location s5:
    edge Drink_Tea goto s9;
    edge Drink_Coffee goto s9;
    edge Starting_Computer goto s7;
  @state(projected: "s5")
  location s6:
    edge Flip_A_Coin goto s7;
  @state(projected: "s6")
  @state(projected: "s9")
  location s7:
    edge Drink_Tea goto s8;
    edge Drink_Coffee goto s8;
  @state(projected: "s11")
  @state(projected: "s10")
  location s8:
    edge Start_Working goto s10;
  @state(projected: "s8")
  @state(projected: "s7")
  location s9:
    edge Starting_Computer goto s8;
  @state(projected: "s12")
  @state(projected: "s13")
  location s10:
    edge __end goto s3;
end
