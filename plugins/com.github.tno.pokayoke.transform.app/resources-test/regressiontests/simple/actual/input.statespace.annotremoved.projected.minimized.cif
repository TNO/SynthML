group Spec:
  controllable Prepare_robot;
  controllable Robot_move_to_location1;
  controllable Robot_move_to_location2;
  controllable Robot_adjust_product;
  controllable Robot_release_product;
  controllable Prepare_location1;
  controllable Measure_product_position;
end
group Post:
  controllable c_satisfied;
end
automaton minimal:
  alphabet Spec.Prepare_robot, Spec.Robot_move_to_location1, Spec.Robot_move_to_location2, Spec.Robot_adjust_product, Spec.Robot_release_product, Spec.Prepare_location1, Spec.Measure_product_position, Post.c_satisfied;
  @state(projected = "s1")
  location s1:
    initial;
    edge Spec.Prepare_location1 goto s2;
    edge Spec.Prepare_robot goto s3;
  @state(projected = "s2")
  location s2:
    edge Spec.Prepare_robot goto s5;
  @state(projected = "s3")
  location s3:
    edge Spec.Prepare_location1 goto s5;
  @state(projected = "s9")
  location s4:
    marked;
  @state(projected = "s4")
  location s5:
    edge Spec.Robot_move_to_location1 goto s6;
  @state(projected = "s5")
  location s6:
    edge Spec.Robot_release_product goto s8;
  @state(projected = "s7")
  location s7:
    edge Spec.Robot_adjust_product goto s8;
    edge Spec.Robot_move_to_location2 goto s9;
  @state(projected = "s6")
  location s8:
    edge Spec.Measure_product_position goto s7;
  @state(projected = "s8")
  location s9:
    edge Post.c_satisfied goto s4;
end
