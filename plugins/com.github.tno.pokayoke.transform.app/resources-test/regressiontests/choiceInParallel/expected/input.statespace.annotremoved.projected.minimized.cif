controllable c_Turn_Computer_On;
controllable c_Starting_Computer;
controllable c_Drink_Coffee;
controllable c_Drink_Tea;
controllable c_Start_Working;
controllable c_Flip_A_Coin;
group Post:
  controllable c_satisfied;
end
automaton minimal:
  alphabet c_Turn_Computer_On, c_Starting_Computer, c_Drink_Coffee, c_Drink_Tea, c_Start_Working, c_Flip_A_Coin, Post.c_satisfied;
  @state(projected = "s1")
  location s1:
    initial;
    edge c_Turn_Computer_On goto s2;
  @state(projected = "s2")
  location s2:
    edge c_Flip_A_Coin goto s4;
    edge c_Starting_Computer goto s5;
  @state(projected = "s13")
  @state(projected = "s14")
  location s3:
    marked;
  @state(projected = "s3")
  location s4:
    edge c_Drink_Tea goto s7;
    edge c_Drink_Coffee goto s7;
    edge c_Starting_Computer goto s6;
  @state(projected = "s4")
  location s5:
    edge c_Flip_A_Coin goto s6;
  @state(projected = "s5")
  @state(projected = "s8")
  location s6:
    edge c_Drink_Tea goto s8;
    edge c_Drink_Coffee goto s8;
  @state(projected = "s7")
  @state(projected = "s6")
  location s7:
    edge c_Starting_Computer goto s8;
  @state(projected = "s10")
  @state(projected = "s9")
  location s8:
    edge c_Start_Working goto s9;
  @state(projected = "s11")
  @state(projected = "s12")
  location s9:
    edge Post.c_satisfied goto s3;
end
