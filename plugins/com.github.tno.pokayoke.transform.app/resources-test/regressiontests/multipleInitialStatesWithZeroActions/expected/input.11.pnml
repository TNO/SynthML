<pnml xmlns="http://www.pnml.org/version-2009/grammar/pnml">
  <net id="minimal" type="http://www.pnml.org/version-2009/grammar/ptnet">
    <page id="minimal">
      <transition id="__start">
        <name>
          <text>__start</text>
        </name>
      </transition>
      <transition id="__end">
        <name>
          <text>__end</text>
        </name>
      </transition>
      <transition id="__reset">
        <name>
          <text>__reset</text>
        </name>
      </transition>
      <place id="p3">
        <name>
          <text>p3</text>
        </name>
      </place>
      <arc id="__end__to__p3" source="__end" target="p3">
      </arc>
      <place id="p1">
        <name>
          <text>p1</text>
        </name>
        <initialMarking>
          <text>1</text>
        </initialMarking>
      </place>
      <arc id="__reset__to__p1" source="__reset" target="p1">
      </arc>
      <place id="p2">
        <name>
          <text>p2</text>
        </name>
      </place>
      <arc id="__start__to__p2" source="__start" target="p2">
      </arc>
      <arc id="p1__to____start" source="p1" target="__start">
      </arc>
      <arc id="p2__to____end" source="p2" target="__end">
      </arc>
      <arc id="p3__to____reset" source="p3" target="__reset">
      </arc>
    </page>
  </net>
