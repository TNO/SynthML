enum UnknownDrink = Coffee, Tea, Unknown;
controllable Turn_Computer_On;
controllable Starting_Computer;
controllable Drink_Coffee;
controllable Drink_Tea;
controllable Start_Working;
controllable Flip_A_Coin;
uncontrollable Flip_A_Coin__result_1;
uncontrollable Flip_A_Coin__result_2;
requirement automaton happens_at_most_once__Turn_Computer_On:
  disc int[0..1] __occurrences;
  marked __occurrences >= 0;
  location:
    initial;
    marked;
    edge Turn_Computer_On when __occurrences < 1 do __occurrences := __occurrences + 1;
end
requirement automaton happens_at_most_once__Drink_Coffee:
  disc int[0..1] __occurrences;
  marked __occurrences >= 0;
  location:
    initial;
    marked;
    edge Drink_Coffee when __occurrences < 1 do __occurrences := __occurrences + 1;
end
requirement automaton happens_at_most_once__Drink_Tea:
  disc int[0..1] __occurrences;
  marked __occurrences >= 0;
  location:
    initial;
    marked;
    edge Drink_Tea when __occurrences < 1 do __occurrences := __occurrences + 1;
end
requirement automaton happens_at_most_once__Start_Working:
  disc int[0..1] __occurrences;
  marked __occurrences >= 0;
  location:
    initial;
    marked;
    edge Start_Working when __occurrences < 1 do __occurrences := __occurrences + 1;
end
requirement automaton happens_at_most_once__Starting_Computer:
  disc int[0..1] __occurrences;
  marked __occurrences >= 0;
  location:
    initial;
    marked;
    edge Starting_Computer when __occurrences < 1 do __occurrences := __occurrences + 1;
end
plant automaton Context:
  disc bool Is_Computer_Ready = false;
  disc bool Is_Computer_On = false;
  disc UnknownDrink Coffee_or_Tea = Unknown;
  disc bool Has_Drank_Coffee_or_Tea = false;
  disc bool Is_Ready_to_Work = false;
  disc int[0..1] __activeAction;
  alg bool postcondition = Is_Computer_On and Is_Computer_Ready and Has_Drank_Coffee_or_Tea and Is_Ready_to_Work;
  alg bool __postcondition__activeAction = __activeAction = 0;
  alg bool __postcondition__happens_at_most_once__Turn_Computer_On = happens_at_most_once__Turn_Computer_On.__occurrences >= 0;
  alg bool __postcondition__happens_at_most_once__Drink_Coffee = happens_at_most_once__Drink_Coffee.__occurrences >= 0;
  alg bool __postcondition__happens_at_most_once__Drink_Tea = happens_at_most_once__Drink_Tea.__occurrences >= 0;
  alg bool __postcondition__happens_at_most_once__Start_Working = happens_at_most_once__Start_Working.__occurrences >= 0;
  alg bool __postcondition__happens_at_most_once__Starting_Computer = happens_at_most_once__Starting_Computer.__occurrences >= 0;
  alg bool __postcondition = postcondition and (__postcondition__activeAction and __postcondition__happens_at_most_once__Turn_Computer_On) and (__postcondition__happens_at_most_once__Drink_Coffee and __postcondition__happens_at_most_once__Drink_Tea and (__postcondition__happens_at_most_once__Start_Working and __postcondition__happens_at_most_once__Starting_Computer));
  marked __postcondition;
  location:
    initial;
    marked;
    edge Turn_Computer_On when not Is_Computer_Ready and not Is_Computer_On, __activeAction = 0 do Is_Computer_On := true;
    edge Starting_Computer when not Is_Computer_Ready and Is_Computer_On, __activeAction = 0 do Is_Computer_Ready := true;
    edge Drink_Coffee when not Has_Drank_Coffee_or_Tea and Coffee_or_Tea = Coffee, __activeAction = 0 do Has_Drank_Coffee_or_Tea := true, Coffee_or_Tea := Unknown;
    edge Drink_Tea when not Has_Drank_Coffee_or_Tea and Coffee_or_Tea = Tea, __activeAction = 0 do Has_Drank_Coffee_or_Tea := true, Coffee_or_Tea := Unknown;
    edge Start_Working when Has_Drank_Coffee_or_Tea and Is_Computer_Ready, __activeAction = 0 do Is_Ready_to_Work := true;
    edge Flip_A_Coin when Is_Computer_On and not Has_Drank_Coffee_or_Tea and Coffee_or_Tea = Unknown, __activeAction = 0 do __activeAction := 1;
    edge Flip_A_Coin__result_1 when __activeAction = 1 do Coffee_or_Tea := Coffee, __activeAction := 0;
    edge Flip_A_Coin__result_2 when __activeAction = 1 do Coffee_or_Tea := Tea, __activeAction := 0;
end
requirement invariant Context.__postcondition disables Turn_Computer_On;
requirement invariant Context.__postcondition disables Starting_Computer;
requirement invariant Context.__postcondition disables Drink_Coffee;
requirement invariant Context.__postcondition disables Drink_Tea;
requirement invariant Context.__postcondition disables Start_Working;
requirement invariant Context.__postcondition disables Flip_A_Coin;
requirement invariant Context.__postcondition disables Flip_A_Coin__result_1;
requirement invariant Context.__postcondition disables Flip_A_Coin__result_2;
