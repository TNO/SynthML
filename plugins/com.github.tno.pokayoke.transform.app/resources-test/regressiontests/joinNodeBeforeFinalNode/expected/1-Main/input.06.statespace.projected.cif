controllable action1;
controllable action2;
controllable __start;
controllable __end;
automaton projected:
  alphabet action1, action2, __start, __end;
  @state(synthesis_state_space: "__init")
  location s1:
    initial;
    edge __start goto s2;
  @state(synthesis_state_space: "loc1")
  location s2:
    edge action2 goto s3;
    edge action1 goto s4;
  @state(synthesis_state_space: "loc3")
  location s3:
    edge action1 goto s6;
  @state(synthesis_state_space: "loc2")
  @state(synthesis_state_space: "loc1")
  @state(synthesis_state_space: "loc4")
  location s4:
    edge action2 goto s5;
    edge action1;
  @state(synthesis_state_space: "loc3")
  @state(synthesis_state_space: "loc6")
  location s5:
    edge __end goto s7;
    edge action1 goto s6;
  @state(synthesis_state_space: "loc5")
  @state(synthesis_state_space: "loc3")
  @state(synthesis_state_space: "loc6")
  location s6:
    edge __end goto s7;
    edge action1;
  @state(synthesis_state_space: "__done")
  location s7:
    marked;
end
