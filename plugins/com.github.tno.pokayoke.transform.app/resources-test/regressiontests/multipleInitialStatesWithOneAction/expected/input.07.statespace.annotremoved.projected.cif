controllable action;
controllable __start;
controllable __end;
automaton projected:
  alphabet action, __start, __end;
  @state(statespace = "__init")
  location s1:
    initial;
    edge __start goto s2;
  @state(statespace = "loc2")
  @state(statespace = "loc1")
  location s2:
    edge __end goto s3;
    edge action goto s4;
  @state(statespace = "__done")
  location s3:
    marked;
  @state(statespace = "loc2")
  location s4:
    edge __end goto s3;
end
