controllable Turn_Computer_On;
controllable Starting_Computer;
controllable Drink_Coffee;
controllable Drink_Tea;
controllable Start_Working;
controllable Flip_A_Coin;
automaton minimal:
  alphabet Turn_Computer_On, Starting_Computer, Drink_Coffee, Drink_Tea, Start_Working, Flip_A_Coin;
  @state(projected = "s1")
  location s1:
    initial;
    edge Turn_Computer_On goto s2;
  @state(projected = "s2")
  location s2:
    edge Flip_A_Coin goto s4;
    edge Starting_Computer goto s5;
  @state(projected = "s11")
  @state(projected = "s12")
  location s3:
    marked;
  @state(projected = "s3")
  location s4:
    edge Drink_Tea goto s7;
    edge Drink_Coffee goto s7;
    edge Starting_Computer goto s6;
  @state(projected = "s4")
  location s5:
    edge Flip_A_Coin goto s6;
  @state(projected = "s5")
  @state(projected = "s8")
  location s6:
    edge Drink_Tea goto s8;
    edge Drink_Coffee goto s8;
  @state(projected = "s7")
  @state(projected = "s6")
  location s7:
    edge Starting_Computer goto s8;
  @state(projected = "s10")
  @state(projected = "s9")
  location s8:
    edge Start_Working goto s3;
end
