controllable c_Turn_Computer_On;
controllable c_Starting_Computer;
controllable c_Drink_Coffee;
controllable c_Drink_Tea;
controllable c_Start_Working;
controllable c_Flip_A_Coin;
group Post:
  controllable c_satisfied;
end
automaton projected:
  alphabet c_Turn_Computer_On, c_Starting_Computer, c_Drink_Coffee, c_Drink_Tea, c_Start_Working, c_Flip_A_Coin, Post.c_satisfied;
  @state(statespace = "loc1")
  location s1:
    initial;
    edge c_Turn_Computer_On goto s2;
  @state(statespace = "loc2")
  location s2:
    edge c_Flip_A_Coin goto s3;
    edge c_Starting_Computer goto s4;
  @state(statespace = "loc4")
  @state(statespace = "loc7")
  @state(statespace = "loc6")
  location s3:
    edge c_Drink_Tea goto s6;
    edge c_Drink_Coffee goto s7;
    edge c_Starting_Computer goto s8;
  @state(statespace = "loc3")
  location s4:
    edge c_Flip_A_Coin goto s5;
  @state(statespace = "loc5")
  @state(statespace = "loc9")
  @state(statespace = "loc8")
  location s5:
    edge c_Drink_Tea goto s9;
    edge c_Drink_Coffee goto s10;
  @state(statespace = "loc11")
  location s6:
    edge c_Starting_Computer goto s9;
  @state(statespace = "loc10")
  location s7:
    edge c_Starting_Computer goto s10;
  @state(statespace = "loc9")
  @state(statespace = "loc8")
  location s8:
    edge c_Drink_Tea goto s9;
    edge c_Drink_Coffee goto s10;
  @state(statespace = "loc13")
  location s9:
    edge c_Start_Working goto s12;
  @state(statespace = "loc12")
  location s10:
    edge c_Start_Working goto s11;
  @state(statespace = "loc14")
  location s11:
    edge Post.c_satisfied goto s13;
  @state(statespace = "loc15")
  location s12:
    edge Post.c_satisfied goto s14;
  @state(statespace = "loc16")
  location s13:
    marked;
  @state(statespace = "loc17")
  location s14:
    marked;
end
