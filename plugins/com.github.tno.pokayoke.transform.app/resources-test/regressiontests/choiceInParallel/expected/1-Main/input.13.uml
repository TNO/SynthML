      <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="64" name="Drink_Coffee">
        <body>not Has_Drank_Coffee_or_Tea and Coffee_or_Tea = Coffee</body>
        <body>Has_Drank_Coffee_or_Tea := true, Coffee_or_Tea:= Unknown</body>
      </ownedBehavior>
      <ownedBehavior xmi:type="uml:OpaqueBehavior" xmi:id="65" name="Drink_Tea">
